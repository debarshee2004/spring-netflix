type Query {
    lolomo: [ShowCategory]
}

type ShowCategory {
   name: String
   shows: [Show]
}

type Show {
    showId: Int
    title: String
    description: String
    categories: [String]
    type: ShowType
    numberOfEpisodes: Int
    duration: String
    artworkUrl: String
}

enum ShowType { SERIES, MOVIE }
